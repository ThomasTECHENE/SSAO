                               );

void main(void)
{
    vec3 c;
    //const int nbSamples = 8; // NOMBRES DE SAMPLES
    vec3 noise[16];
    for (int i = 0; i < 16; i++){
        noise[i] = vec3(pointsNoise[i].x,
                        pointsNoise[i].y,
                        0.0);
        noise[i] = normalize(noise[i]);
    }
    
    //construction du kernel de samples
    vec3 kernel[16];
    for (int i = 0; i < nbSamples; i++){
        kernel[i] = vec3(points[i].x,
                         points[i].y,
                         points[i].z);
        kernel[i] = normalize(kernel[i]);
        kernel[i] *= points[i].z;
        
        float scale = float(i) / float(nbSamples);
        scale = mix(0.1, 1.0, scale * scale);
        kernel[i] *= scale;
    }
    //position espace camera du point d'origine  partir de la depth
    float z = texture2D(linearDepth, texCoord).x;
