     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 330
out vec4 outColor;
in float depth;
in vec4 depthCS;

uniform mat4 matProjection;

void main(void)
{
   float A = matProjection[2].z;
   float B = matProjection[3].z;
   
   float zNear = - B / (1.0 - A);
   float zFar = B / (1.0 + A);
   //valeurs non linaires de profondeur
   float depthFF = 0.5*(-A*depth + B) / depth + 0.5;
   
   //valeurs linaires de profondeur
   float linearDepth = (2.0 * zNear) / (zFar + zNear - depthFF * (zFar - zNear));
   
   //outColor = vec4(depthFF, depthFF, depthFF, 1.0);
   outColor = vec4(linearDepth, linearDepth, linearDepth, 1.0);
   
   
   ///////////////
   /*float d = depthCS.x / depthCS.y;
   outColor = vec4(d,d,d,1.0);*/
}]]></RmShaderCode></RmGLShader>
