    float x = texCoord.x * 2.0 - 1.0;
    float y = texCoord.y * 2.0 - 1.0;
    vec4 projectedPos = vec4(x, y, z, 1.0);
    vec4 positionVS = matProjectionInverse * projectedPos;
    positionVS.xyz /= positionVS.w;
    
    //reconstruction de la normale
    vec3 normal = texture2D(normalTex, texCoord).xyz * 2.0 - 1.0;
    normal = normalize(normal);
    
    //mise a l'echelle pour les coordonnes de texture
    vec2 noiseScale = vec2(512/4, 512/4);
    
    //matrice de changement de base
    int ind = int(floor(texCoord * noiseScale));
