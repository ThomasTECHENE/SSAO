     <RmGLShader NAME="Fragment Program" API="OpenGL" PIXEL_SHADER="TRUE"><RmShaderCode TYPE="TEXT"><![CDATA[
#version 330

uniform sampler2D buffer_normal;
uniform sampler2D buffer_blurAO;
uniform sampler2D buffer_position;
uniform sampler2D metalTex;
uniform float shininess;
uniform vec4 Ia;
uniform vec4 kd;
uniform vec4 Id;
uniform vec4 ks;
uniform vec4 Is;
uniform vec4 lightPos;
uniform vec4 eyePos;


in vec2 texCoord;

out vec4 outColor;

void main(void)
{
   vec3 viewPosition = texture2D(buffer_position, texCoord).xyz;
   vec3 L = lightPos.xyz - viewPosition;
   vec3 V = eyePos.xyz - viewPosition;
   
   L = normalize(L);
   V = normalize(V);
   vec3 N = normalize(texture2D(buffer_normal, texCoord).xyz);
   
   float diffuseIntensity = max( dot( N, L ), 0.0 );
   
   vec3 H = ( L + V ) / 2.0;
   
   float specularIntensity = pow( max( dot( N, H), 0.0), shininess );
   
   float ao = texture2D(buffer_blurAO, texCoord).x;
   vec4 base = texture2D(metalTex, texCoord);
       outColor = ao*base * kd * diffuseIntensity
                + base * ks * specularIntensity;
   
}]]></RmShaderCode></RmGLShader>
