uniform sampler2D occlusionTex;
uniform int blurSize;

in vec2 texCoord;
out vec4 outColor;
void main(void)
{
  
   float ao = texture2D(occlusionTex, texCoord).x;
   float blurResult = 0.0;
   vec2 texSize = 1.0 / vec2(512);
   vec2 h = vec2(float(-blurSize) * 0.5 + 0.5);
   for(int i = 0; i < blurSize; i++){
       for (int j = 0; j < blurSize; j++){
           vec2 offset = (h + vec2(float(i), float(j))) * texSize;
           blurResult += texture2D(occlusionTex, texCoord + offset).x;
       }
   }
   
   outColor = vec4(blurResult / float(blurSize * blurSize));
}]]></RmShaderCode></RmGLShader>
